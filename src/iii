#include <iostream>
#include <fstream>
#include "state.cpp"
#include "io.hpp"

File::File(int a, int b){
	ofstream file("out");
	jd = a; td = b;
	file << td + 1 << " " << jd << endl;
}

void File::out(State& Q){
	for(int j = 0; j < jd; j++){
		file << Q(j,0) << " ";
		file << Q(j,1) << " ";
		file << Q(j,2) << endl;
	} 
}

void File::close(void){
	file.close();
}
